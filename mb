VermelhoAll='\e[01;37;41m'
NADA='\033[0m'
CINZA='\e[02;37m'
DESTACAR='\e[01;37m'
RED='\033[1;31m'
GREEN='\033[1;32m'
YELLOW='\033[1;33m'
SCOLOR='\033[0m'
SEMCOR='\033[8m'
AZUL='\033[36m'
PURPULA='\033[35m'
AMARELO='\033[33m'
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
MenuPrincipal(){
clear
echo -e '\n'
echo -e "         ${PURPULA}================================="
echo -e "                ${GREEN}MB Free @EhisOpeNer    "
echo "        ==================================="
echo -e "               ${AZUL}Qual brinquedo para hoje"
echo
echo "        ===================================" 
echo -e "            ${DESTACAR}[ 1 ] - ${YELLOW}Ganhar MB"
echo -e "            ${DESTACAR}[ 2 ] - ${AMARELO}Instalar pacotes básicos"
echo -e "            ${DESTACAR}[ 0 ] - ${RED}Sair, tenho medo"${AZUL}
echo "        ==================================="
echo

echo 
    read -n1 -p "Raj, escolhe uma carta " App
    case $App in  
    1)Menu;;
    2)clear; echo "instalando pacotes básicos"; sleep 2;
curl -s https://raw.githubusercontent.com/leitura/ospacotesmb/main/packages | xargs pkg install -y; Menu;;
    0) clear ; exit;;
    *) echo ; MenuPrincipal;;
    esac
}
Menu(){
rm -f AccessPassVivo.txt
clear
echo -e '\n'
echo -e "        ${PURPULA}======================================"
echo -e "                  ${GREEN}MB Free @EhisOpeNer    "
echo -e "       ==========================================" 
echo -e "                ${AMARELO}Tá sentido a energia?"
echo
echo -e "      ${PURPULA}============================================="
echo -e "              ${DESTACAR}[ 1 ] - ${AMARELO}Meu AccessPass"
echo -e "              ${DESTACAR}[ 2 ] - ${AMARELO}Usar AccessPass da crush"
echo -e "              ${DESTACAR}[ 3 ] - ${AMARELO}Usar AccessPass memorizados"
echo -e "              ${DESTACAR}[ 4 ] - ${AZUL}Como pegar AccessPass"
echo -e "              ${DESTACAR}[ 0 ] - ${RED}Voltar${NADA}"
echo -e "       ${AZUL}==========================================="
echo 
echo
read -n1 -p "Raj, escolhe uma carta " NovosAds
case $NovosAds in
1)MeuAccessPassVivo ;;
2)AccessPassVivoColado ;;
3)AccessPassVivoColadosSalvos;;
4)ComoPegarAccessPassVivo;;
0)MenuPrincipal;;
*)Menu;;
esac
}

MeuAccessPassVivo(){
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'

echo -e "============================================${NADA}"

echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"

echo -e "============================================${NADA}"

echo -e "${DESTACAR}Sem WI-FI, sem VPN, use dados "
echo -e "móveis limpo, na ${GREEN}Vivo${DESTACAR}, para gerar o ${GREEN}AccessPass${DESTACAR} e então a"
echo -e "diversão começa"

echo

echo -e "Digite ${GREEN}Y${DESTACAR} quando estiver com tesão ${NADA}"

 read -n1 Option005
 case $Option005 in
 Y);;
 y);;
 *)Menu;;
 esac
 
 

AccessPass=$(curl -s -d "pageId=660224&version=2" i.vivo.ddivulga.com/i/gp | cut -d'"' -f4)
echo -e "$AccessPass" >> AccessPassVivo.txt
clear
echo -e "============================================${NADA}"

echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"

echo -e "============================================${NADA}"
echo -e "${DESTACAR}${AZUL}esse é meu garoto, agora vai${NADA}"
sleep 1
ProcurarAds
}

AccessPassVivoColado(){
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
echo -e "============================================${NADA}"

echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"

echo -e "============================================${NADA}"
read -p $'\e[01;33mCole o accessPass: \033[1;32m' AccessPass
echo
SalvarAccessPassVivoColado
}

SalvarAccessPassVivoColado(){
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
echo -e "${NADA}============================================${NADA}"

echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"

echo -e "============================================${NADA}"
read -p $'\e[01;37mmemorizar AccessPass? [Y/n] ' SalvandoAccessPassVivoColado
echo
case $SalvandoAccessPassVivoColado in
n)ProcurarAds;;
N)ProcurarAds;;
Y)read -p $'\e[01;37mpõe um nome legal pro AccessPass: ' PassName ; echo -e " ${GREEN} $PassName${NADA}-$AccessPass" >> AdsSalvos.txt ; ProcurarAds;;
y)read -p $'\e[01;37mpõe um nome legal pro AccessPass: ' PassName ; echo -e "${GREEN} $PassName${NADA}-$AccessPass" >> AdsSalvos.txt ; ProcurarAds;;
*)SalvarAccessPassVivoColado;;
esac
}

ProcurarAds(){
NADA='\033[0m'
CINZA='\e[02;37m'
DESTACAR='\e[01;37m'
SCOLOR='\033[0m'
echo
rm -f advmensagem.txt
rm -f advreward.txt
rm -f advrewardcriar.txt
rm -f advnamecriar.txt
rm -f advname.txt
rm -f TestesFeitos.txt
rm -f AdsEncontrados.txt
rm -f tokencreator.txt
rm -f impressionid.txt
rm -f tokencreator2.txt
rm -f advid.txt
rm -f advidcriar.txt
rm -f impressionid.txt
rm -f impressionidcriar.txt
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
echo -e "${NADA}============================================${NADA}"

echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"

echo -e "============================================${NADA}"
echo -e "${DESTACAR}${AMARELO}Mães solteiras a 39m...${NADA}"


