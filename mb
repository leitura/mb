NovosAdsRev(){
AZUL='\e[01;36;m'
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
clear
echo -e "============================================${NADA}"
echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"
echo -e "============================================${NADA}"

AdsNovos00=$(cat advidcriar.txt |  grep -c ,)

BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'

clear
echo -e "============================================${NADA}"
echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"
echo -e "============================================${NADA}"

case $AdsNovos00 in
1) echo -e "${GREEN}1 Ads novo foi encontrado ${NADA}" ;;
2) echo -e "${GREEN}2 Ads novos foram encontrados ${NADA}" ;;
3) echo -e "${GREEN}3 Ads novos foram encontrados ${NADA}" ;;
4) echo -e "${GREEN}4 Ads novos foram encontrados ${NADA}" ;;
5) echo -e "${GREEN}5 Ads novos foram encontrados ${NADA}" ;;
6) echo -e "${GREEN}6 Ads novos foram encontrados ${NADA}" ;;
7) echo -e "${GREEN}7 Ads novos foram encontrados ${NADA}" ;;
8) echo -e "${GREEN}8 Ads novos foram encontrados ${NADA}" ;;
9) echo -e "${GREEN}9 Ads novos foram encontrados ${NADA}" ;;
10) echo -e "${GREEN}10 Ads novos foram encontrados ${NADA}" ;;
*) echo -e "${RED}se pelo menos 1 ads estivesse disponível  ( ͡° ͜ʖ ͡°)${NADA}" ;;
esac


echo
echo -e "${CINZA}Número:${NADA} ${GREEN}$NumeroDeTelefone${NADA}"
echo -e "${CINZA}ads para ganhar MB:${NADA} ${GREEN}$AdsNovos00"
echo

echo -e "${DESTACAR}1 - buscar mais Ads online${NADA} ${CINZA}socar a Vivo ( ͡° ͜ʖ ͡°)${NADA}"
echo -e "${DESTACAR}2 - Ganhar MB ( ͡° ͜ʖ ͡°)${NADA} ${CINZA}é hora do show${NADA}"
echo -e "${DESTACAR}0 - Voltar${NADA} ${CINZA} usar outro AccessPass ( ͡° ͜ʖ ͡°)${NADA}"
echo
read -p "Raj, escolhe uma carta: " NovosAds
case $NovosAds in
1)GerarMaisAds;;
2)GerarAds;;
0)Menu;;
*)NovosAdsRev;;
esac
} 





NovosAds(){
AdsNovos00=$(cat advidcriar.txt |  grep -c ,)
AdsNovos0=$(cat AdsEncontrados.txt |  grep -c 1)

clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'

echo -e "============================================${NADA}"
echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"
echo -e "============================================${NADA}"

case $AdsNovos0 in
1) echo -e "${GREEN}1 novo Ads foi encontrado${NADA}" ;;
2) echo -e "${GREEN}2 Ads novos foram encontrados${NADA}" ;;
3) echo -e "${GREEN}3 Ads novos foram encontrados${NADA}" ;;
4) echo -e "${GREEN}4 Ads novos foram encontrados${NADA}" ;;
*) echo -e "${RED}Ganhar mais MB  ( ͡° ͜ʖ ͡°)${NADA}" ;;
esac


echo
echo -e "${CINZA}Número:${NADA} ${GREEN}$NumeroDeTelefone${NADA}"
echo -e "${CINZA}ads para ganhar MB:${NADA} ${GREEN}$AdsNovos00"
echo

echo -e "${DESTACAR}1 - buscar mais Ads online${NADA} ${CINZA}socar a Vivo ( ͡° ͜ʖ ͡°)${NADA}"
echo -e "${DESTACAR}2 - Ganhar MB ( ͡° ͜ʖ ͡°)${NADA} ${CINZA}é hora do show${NADA}"
echo -e "${DESTACAR}0 - Voltar${NADA} ${CINZA} usar outro AccessPass ( ͡° ͜ʖ ͡°)${NADA}"
echo
read -p "Raj, escolhe uma carta: " AdsNovos
case $AdsNovos in
1)GerarMaisAds;;
2)GerarAds;;
0)Menu;;
*)NovosAds;;
esac
} 




AccessPassVivoColadosSalvos(){
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'

echo -e "============================================${NADA}"
echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"
echo -e "============================================${NADA}"
AdsSalvos001=$(cat AdsSalvos.txt | grep -n - | grep -c 1:)
    case $AdsSalvos001 in
    1) ;;
    2) ;;
    3) ;;
    *) clear ; echo -e "============================================${NADA}" ; echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}" ; echo -e "============================================${NADA}" ; echo -e "${RED}Ganhar mais MB  ( ͡° ͜ʖ ͡°)${NADA}" ; sleep 1 ; Menu;;
    esac
    
echo -e "${DESTACAR}${AZUL}Raj, escolhe uma carta:${NADA}"
cat AdsSalvos.txt | grep -n - | cut -d'-' -f1
echo
echo -e "0: ${DESTACAR}${RED}apagar todos accessPass memorizados${NADA}"
echo
read -p $'\033[33me aí, qual vai ser? ' NumeroPass
case $NumeroPass in
0)rm -f AdsSalvos.txt ; Menu;;
*) ;;
esac

AccessPass=$(cat AdsSalvos.txt | grep -n - | grep "$NumeroPass": | cut -d':' -f2 | grep -n - | cut -d'-' -f2-9)
echo -e "$AccessPass" >> AccessPassVivo.txt
ProcurarAds
}





ComoPegarAccessPassVivo(){
clear
BrancoAll='\e\033[03;37;47m'
LETRAPRETA='\e\033[02;30;47m'
echo -e "============================================${NADA}"
echo -e "${LETRAPRETA}                  MBS FREE                  ${NADA}"
echo -e "============================================${NADA}"
echo -e "${GREEN}Como pegar AccessPass da crush${NADA}"
echo
echo -e "${DESTACAR}1 - sem VPN, sem WI-FI, usar chip vivo"
echo
echo -e "2- escolhe um modo pra pegar AccessPass"
echo
echo -e "Se for usar pelo Termux:${GREEN}"
cat <<- EOF

clear ; curl -s -d "pageId=660224&version=2" i.vivo.ddivulga.com/i/gp | cut -d '"' -f4 ; echo
EOF
echo
echo -e "${NADA}${DESTACAR}Se for usar pelo navegador:"
echo
echo -e "${GREEN}http://i.vivo.ddivulga.com/i/gp${NADA}"
echo
echo -e "${NADA}${DESTACAR}3 - copie resposta/PASS, recorte como no exemplo abaixo, caso precise, cole no Usar accessPass da crush e salve${NADA}"
echo
echo -e "${GREEN}24IVqUznZQYJxXPe7kqrp3XMAV1Vj5bhDcRSLtWbD_ypLt3PW_4__4Aj2BuUCCpdEOjjtLHmkg_Rk6B_ouI0aw${NADA}"
echo
read -n1 -p "PRESSIONE QUALQUER TECLA PARA CONTINUAR" optione1
  case $optione1 in
  *)Menu;;
  esac
}


